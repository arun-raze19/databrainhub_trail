{
  "version": 2,
  "builds": [
    {
      "src": "api/serverless.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/status.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/test",
      "dest": "/api/test.js"
    },
    {
      "src": "/status",
      "dest": "/api/status.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/serverless.js"
    }
  ]
}
